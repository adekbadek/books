{
  "extends": ["standard", "standard-react", "react-app", "plugin:prettier/recommended"],
  "rules": {
    "comma-dangle": 0,
    "react/prop-types": 0,
    "flowtype/define-flow-type": 1,
    "react/no-unused-prop-types": 0,
  },
  "plugins": [
    "flowtype"
  ],
  "env": {
    "browser": true,
    "jest": true
  },
  "parser": "babel-eslint"
}
