{
  "name": "books-front",
  "version": "0.1.0",
  "private": true,
  "proxy": "http://localhost:3001/",
  "dependencies": {
    "autoprefixer": "7.1.0",
    "babel-core": "6.24.1",
    "babel-loader": "7.0.0",
    "babel-plugin-ramda": "^1.4.3",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-react-app": "3.0.0-alpha.55afd862",
    "babel-preset-stage-0": "^6.24.1",
    "babel-runtime": "6.23.0",
    "case-sensitive-paths-webpack-plugin": "2.0.0",
    "chalk": "1.1.3",
    "classnames": "^2.2.5",
    "color": "^2.0.1",
    "connect-history-api-fallback": "1.3.0",
    "cross-spawn": "4.0.2",
    "css-loader": "0.28.1",
    "dotenv": "4.0.0",
    "extract-text-webpack-plugin": "2.1.0",
    "file-loader": "0.11.1",
    "fs-extra": "3.0.1",
    "history": "^4.7.2",
    "html-webpack-plugin": "2.28.0",
    "lodash.memoize": "^4.1.2",
    "moment": "^2.18.1",
    "node-sass-chokidar": "^0.0.2",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "3.0.0",
    "postcss-loader": "2.0.5",
    "promise": "7.1.1",
    "ramda": "^0.25.0",
    "react": "^15.5.4",
    "react-click-outside": "^2.3.1",
    "react-datepicker": "^0.46.0",
    "react-dev-utils": "1.0.0-alpha.55afd862",
    "react-dom": "^15.5.4",
    "react-error-overlay": "1.0.0-alpha.55afd862",
    "react-loadable": "^3.3.1",
    "react-redux": "^5.0.4",
    "react-router-dom": "^4.1.1",
    "redux": "^3.6.0",
    "style-loader": "0.17.0",
    "sw-precache-webpack-plugin": "0.9.1",
    "tachyons": "^4.7.1",
    "url-loader": "0.5.8",
    "webpack": "2.5.1",
    "webpack-dev-server": "2.4.5",
    "webpack-manifest-plugin": "1.1.0",
    "webpack-stats-plugin": "^0.1.5",
    "whatwg-fetch": "2.0.3"
  },
  "devDependencies": {
    "babel-eslint": "7.2.3",
    "babel-jest": "20.0.3",
    "eslint": "3.19.0",
    "eslint-config-react-app": "1.0.0-alpha.55afd862",
    "eslint-config-standard": "^10.2.1",
    "eslint-config-standard-react": "^5.0.0",
    "eslint-loader": "1.7.1",
    "eslint-plugin-flowtype": "2.33.0",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "^5.0.3",
    "eslint-plugin-node": "^4.2.2",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "7.0.1",
    "eslint-plugin-standard": "^3.0.1",
    "flow-bin": "^0.47.0",
    "flow-typed": "^2.2.3",
    "inquirer": "3.0.6",
    "jest": "20.0.3"
  },
  "scripts": {
    "build-css": "node-sass-chokidar --include-path ./src/styles --include-path ./node_modules src/styles/ -o src/styles/",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src/styles --include-path ./node_modules src/styles/ -o src/styles/ --watch --recursive",
    "start": "NODE_PATH=src node scripts/start.js",
    "build": "npm run build-css && NODE_PATH=src node scripts/build.js",
    "test": "NODE_PATH=src node scripts/test.js --env=jsdom",
    "flow": "flow",
    "flow-typed": "flow-typed install --ignoreDeps dev"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.js?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).js?(x)"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "moduleDirectories": [
      "<rootDir>/src",
      "<rootDir>",
      "node_modules"
    ]
  },
  "babel": {
    "presets": [
      "react-app",
      "babel-preset-stage-0"
    ],
    "plugins": [
      "babel-plugin-transform-decorators-legacy",
      "ramda"
    ]
  }
}
